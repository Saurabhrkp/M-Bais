              <div class="text_info">
                <h1>Explore</h1>
              </div>
            </div>
          </div>
          <div class="courses-sky">
            <div class="inner_courses">
              <div class="text_info">
                <h1>Collabrate</h1>
              </div>
            </div>
          </div>
