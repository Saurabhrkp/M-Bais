</div>
<!-- about_area_end -->

<!-- slider_area_start -->
<div class="slider_area">
  <div
    class="single_slider d-flex align-items-center justify-content-center slider_bg_1"
  >
    <div class="container">
      <div class="row align-items-center justify-content-center">
        <div class="col-xl-12 col-md-12">
